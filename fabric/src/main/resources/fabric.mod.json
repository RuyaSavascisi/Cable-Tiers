  "authors": [
    "Ultramega"
  ],
  "contact": {
    "sources": "https://github.com/starforcraft/Cable-Tiers",
    "issues": "https://github.com/starforcraft/Cable-Tiers/issues"
  },
  "icon": "logo.png",
  "license": "MIT",
  "environment": "*",
