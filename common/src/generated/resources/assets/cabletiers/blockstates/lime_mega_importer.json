{
  "multipart": [
    {
      "apply": {
        "model": "refinedstorage:block/cable/lime"
      }
    },
    {
      "apply": {
        "model": "cabletiers:block/mega_importer",
        "x": 90
      },
      "when": {
        "direction": "down"
      }
    },
    {
      "apply": {
        "model": "cabletiers:block/mega_importer",
        "x": 270
      },
      "when": {
        "direction": "up"
      }
    },
    {
      "apply": {
        "model": "cabletiers:block/mega_importer"
      },
      "when": {
        "direction": "north"
      }
    },
    {
      "apply": {
        "model": "cabletiers:block/mega_importer",
        "x": 180
      },
      "when": {
        "direction": "south"
      }
    },
    {
      "apply": {
        "model": "cabletiers:block/mega_importer",
        "y": 270
      },
      "when": {
        "direction": "west"
      }
    },
    {
      "apply": {
        "model": "cabletiers:block/mega_importer",
        "y": 90
      },
      "when": {
        "direction": "east"
      }
    }
  ]
}